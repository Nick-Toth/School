Cell[TextData[{
 StyleBox["\nExercise 6", "Subsubsection"],
 "\n\nDetermine the first two nonzero terms in the Maclaurin series expansion \
for the following \n\na) ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"cos", "(", "x", ")"}]], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"349f09f4-f546-41b0-ae68-e429568c6f7e"],
 "\nThe first derivative of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"cos", "(", "x", ")"}]], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"93453bed-034e-4f9f-beae-551316a1762f"],
 " is -sin(x)",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"cos", "(", "x", ")"}]], ".", " "}], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"3278359c-4b01-422f-b002-01bfd246d0fa"],
 "\nThe second derivative of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"cos", "(", "x", ")"}]], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"db18dc18-6035-498d-bef6-941b613523c8"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"cos", "(", "x", ")"}]], "(", 
    SuperscriptBox["sin", "2"]}], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"d25d892f-b2f9-4bce-8071-85b11818e835"],
 "(x)",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    RowBox[{"cos", "(", "x", ")"}]}], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"8c454d5b-7b0b-4d42-b706-591b2d195506"],
 ").\n\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "0", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"0", "!"}]], 
   SuperscriptBox["x", "0"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "61abc9a2-c56c-4f06-ac9b-5583258e29a9"],
 " =",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{"f", 
    RowBox[{"(", "0", ")"}]}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "42c3b1b9-9314-46d4-8dad-bb273784e935"],
 "= ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["e", 
    RowBox[{"cos", 
     RowBox[{"(", "0", ")"}]}]], "=", 
   RowBox[{
    SuperscriptBox["e", "1"], "=", 
    RowBox[{"e", "."}]}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "c5114df3-fca0-4736-9164-ec1c846cf193"],
 "\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "1", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"1", "!"}]], 
   SuperscriptBox["x", "1"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "de158801-7cb1-47cf-aaa0-6bfa0e961b8f"],
 " =",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{
    SuperscriptBox["xf", 
     RowBox[{"(", "1", ")"}]], 
    RowBox[{"(", "0", ")"}]}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "7ddfdd4a-0d0f-4130-9fab-ea60dd77660b"],
 "= -xsin(0)",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{"cos", "(", "0", ")"}]], "=", "0"}], ",", "  ", 
    RowBox[{
     RowBox[{"since", " ", 
      RowBox[{"sin", "(", "0", ")"}]}], "=", "0.", " "}]}], TraditionalForm]],
  
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"840e6d5b-68a9-4f7b-8a37-786c15dd7256"],
 "\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "2", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"2", "!"}]], 
   SuperscriptBox["x", "2"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "3765c634-7ffd-4443-a53a-2f542724c323"],
 " =",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["x", "2"], " "}], "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "0", ")"}]}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "76e88919-5195-487d-9e7e-81d47ac7672c"],
 "= ",
 Cell[BoxData[
  FractionBox[
   RowBox[{
    SuperscriptBox["x", "2"], " "}], "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "1a59b032-26d3-47e7-abda-01a392a39cd6"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["e", 
     RowBox[{"cos", "(", "0", ")"}]], "(", 
    SuperscriptBox["sin", "2"]}], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"7e6183f1-6aad-415c-a081-2f2dea4cfbab"],
 "(0)",
 Cell[BoxData[
  FormBox[
   RowBox[{"-", 
    RowBox[{"cos", "(", "0", ")"}]}], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"7a4ba998-e83f-4669-b66d-7439bca641f8"],
 ") = -",
 Cell[BoxData[
  FractionBox[
   RowBox[{"e", " "}], "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "1779c770-b6c1-44ac-84b0-799f84ffd628"],
 Cell[BoxData[
  SuperscriptBox["x", "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "eecaf894-3f85-4c5c-ad16-63ea684d805a"],
 ".\n\nSo the two term approximation for the Maclaurin series of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"cos", "(", "x", ")"}]], TraditionalForm]],
  CellChangeTimes->{{3.787443562370092*^9, 3.78744356659278*^9}},
  ExpressionUUID->"4e2a40d4-4d27-41c6-b576-762b3e95497a"],
 " is: e -",
 Cell[BoxData[
  FractionBox[
   RowBox[{"e", " "}], "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "1c79c8b3-97cc-4055-96ee-67ea624a5f73"],
 Cell[BoxData[
  SuperscriptBox["x", "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "1a806592-7a14-4ffa-bb5b-493a5682b15e"],
 " = e",
 Cell[BoxData[
  FractionBox[
   RowBox[{"2", "-", 
    SuperscriptBox["x", "2"]}], "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "27fc799d-c17d-4cfe-9c24-326970f804a2"],
 ".\n\nb) sin(cos(x))\nThe first derivative of sin(cos(x)) is \
-sin(x)cos(cos(x))\nThe second derivative of sin(cos(x)) is \
-cos(x)cos(cos(x)) - ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["sin", "2"], 
   RowBox[{"(", "x", ")"}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "125a3b00-64ff-43b5-b230-4e37a443fb3e"],
 "sin(cos(x)).\n\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "0", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"0", "!"}]], 
   SuperscriptBox["x", "0"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "a3f3876c-a0e7-49c3-a30c-46bab8aad973"],
 "= f(0) = sin(cos(0)) = sin(1).\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "1", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"1", "!"}]], 
   SuperscriptBox["x", "1"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "9cdb5408-aa69-4f1c-a9f9-c3d56d8c32cb"],
 " = ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["xf", 
    RowBox[{"(", "1", ")"}]], 
   RowBox[{"(", "0"}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "6e8dff35-8508-45aa-a88b-f0d5db8607ea"],
 ") = -x(sin(0)cos(cos(0))) = 0 since sin(0) = 0.\n",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "2", ")"}]], "(", "0", ")"}], " "}], 
    RowBox[{"2", "!"}]], 
   SuperscriptBox["x", "2"]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "6b6dbbe4-3760-49e8-89a3-e48b7de64007"],
 " = ",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox["x", "2"], " "}], "2"], 
   SuperscriptBox["f", 
    RowBox[{"(", "2", ")"}]], 
   RowBox[{"(", "0"}]}]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "8c1741c9-dc6c-4772-af09-3bb08c8a3a17"],
 ") = -",
 Cell[BoxData[
  FractionBox[
   RowBox[{
    SuperscriptBox["x", "2"], " "}], "2"]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
