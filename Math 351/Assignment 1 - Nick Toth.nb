   RowBox[{
    FormBox[
     FractionBox[
      RowBox[{"sin", "(", 
       RowBox[{"2", "x"}], ")"}], "2"],
     TraditionalForm], "=", 
    RowBox[{
     FormBox[
      FractionBox["1", "2"],
      TraditionalForm], 
     UnderoverscriptBox[
      RowBox[{"\[Sum]", " "}], 
      RowBox[{" ", 
       RowBox[{"k", "=", "0"}]}], "\[Infinity]"], 
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
