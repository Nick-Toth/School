54a9ffa41fb4"],

Cell[TextData[{
 StyleBox["\nExercise 14", "Subsubsection"],
 "\n\nWrite the Taylor series for the function f(x) =",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     SuperscriptBox["x", "3"], "-", 
     RowBox[{"2", 
      SuperscriptBox["x", "2"]}], "+", " ", 
     RowBox[{"4", "x"}], " ", "-", " ", "1"}]}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "d8290a9e-fa75-412b-82d3-43ad9918d07d"],
 " about x=2.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"(", "0", ")"}]], "(", 
    SubscriptBox["x", "0"], " ", ")"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "977b6c1a-4bf2-4453-94a3-74a4344e0fbb"],
 " = f(",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", "0"], " "}]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "54cd391e-b8e7-45d8-afa6-82906c96edf6"],
 ")\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"(", "1", ")"}]], "(", 
    SubscriptBox["x", "0"], " ", ")"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "e371b8bf-4016-4a4a-9e51-112350aaa364"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"3", 
     SuperscriptBox[
      RowBox[{"(", 
       SubscriptBox["x", "0"], ")"}], "2"]}], "-", 
    RowBox[{"4", 
     SubscriptBox["x", "0"]}], "+", "4"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "46ec4153-e5f3-4c46-a23c-647900bde586"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], "(", 
    SubscriptBox["x", "0"], " ", ")"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "c7a45475-580a-4b8f-bf4b-1db37e573a7a"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"6", 
     RowBox[{"(", 
      SubscriptBox["x", "0"], ")"}]}], "-", "4"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "dff8b960-3337-4ca3-98a4-2fa4244c12b4"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], "(", 
    SubscriptBox["x", "0"], " ", ")"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "ebdcce2f-42bf-4949-ac44-4175031a4cf0"],
 " = ",
 Cell[BoxData[
  FormBox["6", TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "a9f7f698-e1a9-4fee-8b53-22a5053ba59b"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"(", "k", ")"}]], "(", 
    SubscriptBox["x", "0"], " ", ")"}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "8012c8f2-6b81-4dd9-b4f9-41ee41e8b9f3"],
 " = 0 for all k \[GreaterEqual] 4\n\n",
 Cell[BoxData[
  SubscriptBox["x", "0"]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "be8502a2-2d08-4f42-aec4-e64dd8d004c8"],
 " = 2, so",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     UnderoverscriptBox[
      RowBox[{"\[Sum]", " "}], 
      RowBox[{" ", 
       RowBox[{"k", "=", "0"}]}], "\[Infinity]"], 
     FractionBox[
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "k", ")"}]], "(", 
       SubscriptBox["x", "0"], " ", ")"}], 
      RowBox[{"k", "!"}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"x", "-", 
        SubscriptBox["x", "0"]}], ")"}], "k"]}]}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "24bf5519-3666-460a-961e-fd35aa8f3fa8"],
 "  = ",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "2", ")"}]}], "+", 
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", "2", ")"}], 
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}]}], "+"}]}]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "6d5ca256-095c-48bd-a59d-8df7c9619bed"],
 " ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "''"}], 
     RowBox[{"(", "2", ")"}]}], "2"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "ce30f974-4ddb-4a6f-b60b-1f59be3b6972"],
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}],
     TraditionalForm], "2"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "734705c9-8a80-4bfc-9a8a-737692f504e2"],
 "  + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "'''"}], 
     RowBox[{"(", "2", ")"}]}], "6"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "17f3d13c-992f-4180-9678-3e40f5689331"],
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}],
     TraditionalForm], "3"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "a83a8b35-3431-40e3-a0a6-599a1a56d3ff"],
 " + 0\n                                                          = ",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{"7", "+", 
    RowBox[{"8", 
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}]}], "+"}]}]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "6103dbc8-7673-4c98-b02f-30d444f6a717"],
 " 4",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}],
     TraditionalForm], "2"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "33d9d1e2-a70a-43a4-8c2b-0e967b36131e"],
 "  + ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{" ", 
      RowBox[{"(", 
       FormBox[
        RowBox[{"x", "-", "2"}],
        TraditionalForm], ")"}]}],
     TraditionalForm], "3"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "ca06d2e7-470e-46e9-ac88-a64503a200e9"],
 "\n\nTherefore the Taylor series for the function f(x) =",
 Cell[BoxData[
  FormBox[
   RowBox[{"  ", 
    RowBox[{
     SuperscriptBox["x", "3"], "-", 
     RowBox[{"2", 
      SuperscriptBox["x", "2"]}], "+", " ", 
     RowBox[{"4", "x"}], " ", "-", " ", "1"}]}], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "053493d8-607e-4b1b-868d-ca14da9fe89c"],
 " about x=2 is (in its reduced form) ",
 Cell[BoxData[
  RowBox[{" ", 
   RowBox[{"7", "+", 
    RowBox[{"8", 
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", "-", "2"}],
       TraditionalForm], ")"}]}], "+"}]}]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "f7148243-2c57-47ed-882a-3b2402b648c6"],
 "4",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", " ", "-", " ", "2"}],
       TraditionalForm], ")"}],
     TraditionalForm], "2"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "15794213-895e-4d4a-9667-9fba55b5d513"],
 "  + ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    FormBox[
     RowBox[{"(", 
      FormBox[
       RowBox[{"x", " ", "-", " ", "2"}],
       TraditionalForm], ")"}],
     TraditionalForm], "3"], TraditionalForm]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "82a21cd4-ee0c-4a1d-903e-daf67fe3a565"],
 "."
}], "Text",
 CellChangeTimes->{{3.7876232974605513`*^9, 3.7876233207614937`*^9}, {
   3.7876242164371367`*^9, 3.78762427491562*^9}, {3.787628998309807*^9, 
   3.78762900104424*^9}, {3.78762908209798*^9, 3.787630057706645*^9}, {
   3.787630132647562*^9, 3.7876303199575033`*^9}, {3.787630350509035*^9, 
   3.7876304780064907`*^9}, {3.787630525207305*^9, 3.7876307738118896`*^9}, 
   3.7877852502429113`*^9, {3.788046008856307*^9, 3.788046012318734*^9}, {
   3.788103106349564*^9, 3.788103185980307*^9}, {3.788103262817245*^9, 
   3.788103920872796*^9}, {3.788104066836726*^9, 3.788104162434168*^9}, {
   3.7881042580322657`*^9, 3.788104329726137*^9}, {3.788127008711706*^9, 
   3.788127015696381*^9}},ExpressionUUID->"948c970b-f343-430b-8f72-\
8c9270ba9e4e"],

Cell[TextData[{
 StyleBox["\nExercise 15", "Subsubsection"],
 "\n\nDetermine the first four non-zero terms in the series expansion about \
zero for\n\nNote first that  ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"sin", 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    UnderoverscriptBox[
     RowBox[{"\[Sum]", " "}], 
     RowBox[{" ", 
      RowBox[{"k", "=", "0"}]}], "\[Infinity]"], 
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "k"], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "k"}], "+", "1"}], ")"}], "!"}]], 
    SuperscriptBox["x", 
     RowBox[{
      RowBox[{"2", "k"}], "+", "1"}]]}]}]],
  CellChangeTimes->{{3.787449318086979*^9, 3.787449375001822*^9}, {
   3.7874504172403297`*^9, 3.7874504712522793`*^9}},ExpressionUUID->
  "f417d34a-534b-440d-b243-ef22176e91de"],
