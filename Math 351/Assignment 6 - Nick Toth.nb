        TraditionalForm], "2"]}], "-", 
     FormBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", 
        SubscriptBox["x", "1"]}], ")"}],
      TraditionalForm]}], ")"}]}]],
  CellChangeTimes->{3.791667015467074*^9},ExpressionUUID->
  "776c2260-61c2-4841-a0ec-39954333a688"],
 "\n                    = ",
 Cell[BoxData[
  FractionBox[
   RowBox[{
    FormBox[
     SubscriptBox["y", "1"],
     TraditionalForm], "-", 
    FormBox[
     SubscriptBox["y", "0"],
     TraditionalForm]}], 
   RowBox[{
    FormBox[
     SubscriptBox["x", "1"],
     TraditionalForm], "-", 
    FormBox[
     SubscriptBox["x", "0"],
     TraditionalForm]}]]],
  CellChangeTimes->{3.791667015467074*^9},ExpressionUUID->
  "fdf5f5e8-c369-46bd-96fb-5a46e13cb9e9"],
 "\n                    = ",
 Cell[BoxData[
  FractionBox[
   RowBox[{
    FormBox[
     RowBox[{"f", "(", 
      FormBox[
       SubscriptBox["x", "1"],
       TraditionalForm], ")"}],
     TraditionalForm], "-", 
    FormBox[
     RowBox[{"f", "(", 
      FormBox[
       SubscriptBox["x", "0"],
       TraditionalForm], ")"}],
     TraditionalForm]}], 
   RowBox[{
    FormBox[
     SubscriptBox["x", "1"],
     TraditionalForm], "-", 
