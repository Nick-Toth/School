  CellChangeTimes->{{3.7916674447002068`*^9, 3.791667483171034*^9}},
  ExpressionUUID->"1cbee5bc-263e-4338-9e39-f92942868a43"],
 "+",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "3"], "[", 
    RowBox[{
     FractionBox["22", "5"], "-", 
     FractionBox["24", "5"]}], "]"}], TraditionalForm]],
  CellChangeTimes->{{3.7916674447002068`*^9, 3.791667483171034*^9}},
  ExpressionUUID->"39e2638c-ee47-49d1-ace5-3c785a131c6d"],
