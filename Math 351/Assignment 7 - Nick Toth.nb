  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"a7c76d4f-da51-44dd-89b1-a69ef1c67e9e"],
 " + 0, since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "4"], "[", 
    RowBox[{"1", "+", 
     SuperscriptBox["e", 
      RowBox[{"-", 
       FormBox["100",
        TraditionalForm]}]]}], "]"}], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"5cf726fe-d43b-45fc-aa48-8f4105ee3f01"],
 " \[TildeTilde] ",
 Cell[BoxData[
  FractionBox["1", "4"]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"af80ab78-6b3f-46a0-8df3-a6764caadd4e"],
 " and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{"-", 
     FormBox["50",
      TraditionalForm]}]], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"5a1f58c0-929d-4540-b1a8-7c47ff6e4602"],
 "\[TildeTilde] 0.\n\nFinally, R(1,1) = R(1,0) + ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{
      FormBox[
       SuperscriptBox["4", "1"],
       TraditionalForm], "-", "1"}]], "[", 
    RowBox[{
     RowBox[{"R", "(", 
      RowBox[{"1", ",", "0"}], ")"}], "-", 
     RowBox[{"R", "(", 
      RowBox[{"0", ",", "0"}], ")"}]}], "]"}], TraditionalForm]],
  CellChangeTimes->{{3.7916674447002068`*^9, 3.791667483171034*^9}},
  ExpressionUUID->"d7a3666e-f6c5-4efb-ba0f-ee5b406987e6"],
 "\n                             = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "4"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"11c96f42-ab1f-4a2c-b264-843950d70b74"],
 " + ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "3"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"3cfa9975-7868-4ded-a576-da6f0a381458"],
 "[",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "4"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"b566498c-b60b-4fe3-b6c4-6015f9546557"],
 "-",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"a0651022-af8a-4752-aaa0-70d4f47c2f4d"],
 "]\n                             = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "4"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"cc5e131e-bef0-4f21-a6f1-7c96a3710ae4"],
 " -",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "12"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"7b632ee5-01fb-4946-9d66-fb4a3397a6da"],
 "\n                             = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"12", "-", "4"}], 
    RowBox[{"4", "*", "12"}]], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"def4cd29-0f7b-4124-9979-bba315cddd8c"],
 "\n                             = ",
 Cell[BoxData[
  FormBox[
   FractionBox["8", "48"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"f6a23fec-5c14-47ad-833d-95b0dc51bf94"],
 "\n                             = ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "6"], TraditionalForm]],
  CellChangeTimes->{{3.791667904013838*^9, 3.791667920281123*^9}},
  ExpressionUUID->"de0ab876-8aa0-41af-9263-b01cae0345e3"],
 "."
