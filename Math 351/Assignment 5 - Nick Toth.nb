Cell[TextData[{
 "\n\n",
 StyleBox["Exercise 2", "Subsubsection",
  FontSize->24],
 StyleBox["\n\n", "Subsubsection"],
 StyleBox["Using a Taylor series, establish the error term for the formula ", 
  "Chapter",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", 
      StyleBox["0", "Chapter",
       FontSize->18], ")"}]}], "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   StyleBox["\[TildeTilde]", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"2", "h"}], ")"}]}], " ", "-", " ", 
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.790380970184108*^9},
  FontSize->18,ExpressionUUID->"52656544-9224-436d-9016-2461bc546a76"],
 StyleBox[".\n\n",
  FontSize->18],
 "\n ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"f", 
    RowBox[{"(", 
     RowBox[{"2", "h"}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}], "+", 
     RowBox[{"2", 
      RowBox[{"hf", "'"}], 
      RowBox[{"(", "x", ")"}]}], "+", 
     RowBox[{"4", 
      SuperscriptBox["h", "2"], 
      FractionBox[
       RowBox[{
        RowBox[{"f", "''"}], 
        RowBox[{"(", "\[Xi]", ")"}]}], "2"]}]}], "=", 
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}], "+", 
     RowBox[{"2", "h", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x", ")"}]}], "+", 
        RowBox[{
         RowBox[{"hf", "''"}], 
         RowBox[{"(", "\[Xi]", ")"}]}]}], ")"}]}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "eec07e4e-b85e-47ad-886d-ff1026138b2d"],
 ".\n \n ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", 
      RowBox[{"2", "h"}], ")"}]}], "-", 
    RowBox[{"f", 
     RowBox[{"(", "0", ")"}]}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}], "+", 
     RowBox[{"2", "h", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x", ")"}]}], "+", 
        RowBox[{
         RowBox[{"hf", "''"}], 
         RowBox[{"(", "\[Xi]", ")"}]}]}], ")"}]}], "-", 
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}]}], " ", "=", 
    RowBox[{"2", "h", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "'"}], 
         RowBox[{"(", "x", ")"}]}], "+", 
        RowBox[{
         RowBox[{"hf", "''"}], 
         RowBox[{"(", "\[Xi]", ")"}]}]}], ")"}], ".", " "}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "a39388ce-753f-409d-9f68-19ca1e6f1bbc"],
 "\n\n So ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", "'"}], 
    RowBox[{"(", "x", ")"}]}], "\[TildeTilde]", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"f", 
       RowBox[{"(", 
        RowBox[{"2", "h"}], ")"}]}], "-", 
      RowBox[{"f", 
       RowBox[{"(", "0", ")"}]}]}], 
     RowBox[{"2", "h"}]], "-", 
    RowBox[{
     RowBox[{"hf", "''"}], 
     RowBox[{"(", "\[Xi]", ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "8349a339-db30-4539-9094-fec038191c4b"],
 ".\n\n Therefore ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"-", 
    RowBox[{"hf", "''"}]}], 
   RowBox[{"(", "\[Xi]", ")"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "4fff2a87-22e4-43c4-bc59-f83663894079"],
 " is the error term for ",
 Cell[BoxData[
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", 
      StyleBox["0", "Chapter",
       FontSize->18], ")"}]}], "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   StyleBox["\[TildeTilde]", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"2", "h"}], ")"}]}], " ", "-", " ", 
     RowBox[{"f", 
      RowBox[{"(", "0", ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "c4997413-e4d1-4ac5-a8af-b97e8d79b0b9"],
 "."
}], "Text",
