Cell[TextData[{
 StyleBox["\nSo our approximation is correct to about 8.1*",
  FontSize->16],
 Cell[BoxData[
  SuperscriptBox["10", 
   RowBox[{"-", "6"}]]],
  CellChangeTimes->{{3.790724394557126*^9, 3.790724410739637*^9}},
