Cell[TextData[{
 "\n",
 StyleBox["Exercise 8", "Subsubsection",
  FontSize->24],
 "\n",
 StyleBox["\n", "Subsubsection",
  FontSize->24],
 "Derive the two formulas:\n    a) ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SuperscriptBox["f", "'"], 
    RowBox[{"(", "x", ")"}]}], "\[TildeTilde]", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"4", "h"}]], "[", 
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "+", 
        RowBox[{"2", "h"}]}], ")"}]}], " ", "-", " ", 
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "-", 
        RowBox[{"2", "h"}]}], ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.7906165341057777`*^9},ExpressionUUID->
  "00bcf279-2a7c-45e1-b11c-f21579bf6047"],
 ".\n    \n         Derivation:\n              ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", 
      RowBox[{"x", "+", 
       RowBox[{"2", "h"}]}], ")"}]}], "-", 
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}]}], "=", 
   RowBox[{
    RowBox[{"2", 
     RowBox[{"hf", "'"}], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["4", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["8", 
      RowBox[{"3", "!"}]], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", 
      SubscriptBox["\[Xi]", "1"], ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "51be9f0e-9a1e-4137-b109-a66f7762d0d0"],
 ",\n              ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", 
      RowBox[{"x", "-", 
       RowBox[{"2", "h"}]}], ")"}]}], "-", 
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "2"}], 
     RowBox[{"hf", "'"}], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["4", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "-", 
    RowBox[{
     FractionBox["8", 
      RowBox[{"3", "!"}]], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", 
      SubscriptBox["\[Xi]", "2"], ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "a392472d-f0ab-4e30-baf6-8c5d8f74a000"],
 ".\n\n              Then\n              (f(x+2h)-f(x)) - (f(x-2h)-f(x)) = \
f(x+2h)-f(x-2h)\n                                                             \
        = (",
 Cell[BoxData[
  RowBox[{
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "1"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "2dd7de30-81ee-4a80-ace2-18e94bbc9436"],
 ") - (",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"-", "2"}], 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "2"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "63cbd695-6b7d-4d06-af90-d8799fcde991"],
 ") \n                                                                     = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "1"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "7a2266bf-65fc-437a-92d2-12ae21d4fbfd"],
 " ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"+", "2"}], 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], "'"}], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "2"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "9a7217a8-8e58-4a91-b4ba-9d957589c439"],
 "\n                                                                     = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    RowBox[{
     SuperscriptBox["h", "3"], "[", 
     RowBox[{
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", 
        SubscriptBox["\[Xi]", "1"], ")"}]}], "+", 
