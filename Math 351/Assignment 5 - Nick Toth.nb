     FractionBox["4", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["8", 
      RowBox[{"3", "!"}]], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["16", 
      RowBox[{"4", "!"}]], 
     SuperscriptBox["h", "4"], 
     SuperscriptBox["f", 
      RowBox[{"(", "4", ")"}]], 
     RowBox[{"(", 
      SubscriptBox["\[Xi]", "1"], ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "cfd02433-01e3-406f-add6-90fa662745c7"],
 ",\n              ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", 
      RowBox[{"x", "-", 
       RowBox[{"2", "h"}]}], ")"}]}], "-", 
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "2"}], 
     RowBox[{"hf", "'"}], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["4", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "-", 
    RowBox[{
     FractionBox["8", 
      RowBox[{"3", "!"}]], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["16", 
      RowBox[{"4", "!"}]], 
     SuperscriptBox["h", "4"], 
     SuperscriptBox["f", 
      RowBox[{"(", "4", ")"}]], 
     RowBox[{"(", 
      SubscriptBox["\[Xi]", "2"], ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "e26f2a2c-978d-401f-a9fb-ac2283e02e60"],
 ".\n              \n              f(x+2h)-f(x) + f(x-2h)-f(x) = f(x+2h) + \
f(x-2h)-2f(x)\n                                                               \
 = (",
 Cell[BoxData[
  RowBox[{
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["16", 
     RowBox[{"4", "!"}]], 
    SuperscriptBox["h", "4"], 
    SuperscriptBox["f", 
     RowBox[{"(", "4", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "1"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "53e4fd4c-d7ca-4e1d-9cdd-8db3e141c765"],
 ") + (",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"-", "2"}], 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["16", 
     RowBox[{"4", "!"}]], 
    SuperscriptBox["h", "4"], 
    SuperscriptBox["f", 
     RowBox[{"(", "4", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "2"], ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "5e420172-c0ff-42f5-979c-a4e2fc511083"],
 ")\n                                                                = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
