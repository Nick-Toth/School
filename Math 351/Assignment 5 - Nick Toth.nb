Cell[TextData[{
 StyleBox["\nExercise 3\n\n", "Subsubsection",
  FontSize->24],
 StyleBox["Derive the approximation formula ", "Chapter",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", 
      StyleBox["x", "Chapter",
       FontSize->18], ")"}]}], "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   StyleBox["\[TildeTilde]", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"4", "f", 
      RowBox[{"(", 
       RowBox[{"x", "+", "h"}], ")"}]}], " ", "-", " ", 
     RowBox[{"3", "f", 
      RowBox[{"(", "x", ")"}]}], "-", 
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "+", 
        RowBox[{"2", "h"}]}], ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "ba816ff7-c9be-49e1-bb86-8e99ca436019"],
 StyleBox[".\nand show that its error term is of the form ",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   FractionBox["1", "3"], 
   SuperscriptBox["h", "2"], 
   SuperscriptBox["f", 
    RowBox[{"(", "3", ")"}]], 
   RowBox[{"(", "\[Xi]", ")"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "3f1b1170-05ec-4966-8d6a-d1c10982ddc3"],
 ".\n\n    Derivation:\n        ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"f", 
    RowBox[{"(", 
     RowBox[{"x", "+", 
      RowBox[{"2", "h"}]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{"2", 
     RowBox[{"hf", "'"}], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["4", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["8", "6"], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", "\[Xi]", ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "ac3c2053-6f53-42a6-b98a-f4e4699197fe"],
 ", \n        ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"f", 
    RowBox[{"(", 
     RowBox[{"x", "+", "h"}], ")"}]}], "    ", "=", 
   RowBox[{
    RowBox[{"f", 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     RowBox[{"hf", "'"}], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "2", ")"}]], 
     RowBox[{"(", "x", ")"}]}], "+", 
    RowBox[{
     FractionBox["1", "6"], 
     SuperscriptBox["h", "3"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", "\[Xi]", ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "62be7920-fcc9-43e6-99ad-80f8a1654b25"],
 ".\n\n        Observe that ",
 Cell[BoxData[
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", 
      StyleBox["x", "Chapter",
       FontSize->18], ")"}]}], "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   StyleBox["\[TildeTilde]", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"4", "f", 
      RowBox[{"(", 
       RowBox[{"x", "+", "h"}], ")"}]}], " ", "-", " ", 
     RowBox[{"3", "f", 
      RowBox[{"(", "x", ")"}]}], "-", 
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "+", 
        RowBox[{"2", "h"}]}], ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "764e9c7c-15be-4a91-a0ac-fb3263ce745f"],
 "\n                                                ",
 Cell[BoxData[
  RowBox[{"=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"4", "f", 
      RowBox[{"(", 
       RowBox[{"x", "+", "h"}], ")"}]}], "-", 
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "+", 
        RowBox[{"2", "h"}]}], ")"}]}], "-", " ", 
     RowBox[{"3", "f", 
      RowBox[{"(", "x", ")"}]}]}], "]"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "57e04de3-3e52-4127-a2d6-43e6211d6188"],
 " \n\n        4f(x+h)-f(x+2h) = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"f", 
       RowBox[{"(", "x", ")"}]}], "+", 
      RowBox[{
       RowBox[{"hf", "'"}], 
       RowBox[{"(", "x", ")"}]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["h", "2"], 
       SuperscriptBox["f", 
        RowBox[{"(", "2", ")"}]], 
       RowBox[{"(", "x", ")"}]}], "+", 
      RowBox[{
       FractionBox["1", "6"], 
       SuperscriptBox["h", "3"], 
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", "\[Xi]", ")"}]}]}], ")"}]}], "-", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"f", 
      RowBox[{"(", "x", ")"}]}], "+", 
     RowBox[{"2", 
      RowBox[{"hf", "'"}], 
      RowBox[{"(", "x", ")"}]}], "+", 
     RowBox[{
      FractionBox["4", "2"], 
      SuperscriptBox["h", "2"], 
      SuperscriptBox["f", 
       RowBox[{"(", "2", ")"}]], 
      RowBox[{"(", "x", ")"}]}], "+", 
     RowBox[{
      FractionBox["8", "6"], 
      SuperscriptBox["h", "3"], 
      SuperscriptBox["f", 
       RowBox[{"(", "3", ")"}]], 
      RowBox[{"(", "\[Xi]", ")"}]}]}], ")"}], "-", " ", 
   RowBox[{"3", "f", 
    RowBox[{"(", "x", ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "1024412b-10c7-44d2-8451-e2f047f423e1"],
 "\n                                        = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", "f", 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{"4", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "6"], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "\[Xi]", ")"}]}], "-", 
   RowBox[{"f", 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["8", "6"], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "\[Xi]", ")"}]}], "-", " ", 
   RowBox[{"3", "f", 
    RowBox[{"(", "x", ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "e218c0c4-97e5-4900-8b15-f2e4a81760f1"],
 "\n                                        = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", "f", 
    RowBox[{"(", "x", ")"}]}], "-", " ", 
   RowBox[{"3", "f", 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{"f", 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{"4", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["4", "2"], 
    SuperscriptBox["h", "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["4", "6"], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "\[Xi]", ")"}]}], "-", 
   RowBox[{
    FractionBox["8", "6"], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "\[Xi]", ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "b21de058-ecd5-4c0a-a55e-e028f9201b91"],
 "\n                                        = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"2", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox["4", "6"], 
    SuperscriptBox["h", "3"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", "\[Xi]", ")"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "06e8cfc4-e749-4d61-a0be-8c884d2d5460"],
 ".\n\n        So ",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    RowBox[{
     RowBox[{"4", "f", 
      RowBox[{"(", 
       RowBox[{"x", "+", "h"}], ")"}]}], "-", 
     RowBox[{"3", "f", 
      RowBox[{"(", "x", ")"}]}], "-", 
     RowBox[{"f", 
      RowBox[{"(", 
       RowBox[{"x", "+", 
        RowBox[{"2", "h"}]}], ")"}]}]}], 
    RowBox[{"2", "h"}]], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"2", 
       RowBox[{"hf", "'"}], 
       RowBox[{"(", "x", ")"}]}], "-", 
      RowBox[{
       FractionBox["4", "6"], 
       SuperscriptBox["h", "3"], 
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", "\[Xi]", ")"}]}]}], 
     RowBox[{"2", "h"}]], "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", "x", ")"}]}], "-", 
      RowBox[{
       FractionBox["4", 
        RowBox[{"12", "h"}]], 
       SuperscriptBox["h", "3"], 
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", "\[Xi]", ")"}]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", "x", ")"}]}], "+", 
      RowBox[{
       FractionBox["1", "3"], 
       SuperscriptBox["h", "2"], 
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", "\[Xi]", ")"}]}]}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "7e99ad83-15a5-475d-aa5c-1989476b1cb0"],
 ".\n        And finally ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"f", "'"}], 
    RowBox[{"(", "x", ")"}]}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"4", "f", 
       RowBox[{"(", 
        RowBox[{"x", "+", "h"}], ")"}]}], "-", 
      RowBox[{"3", "f", 
       RowBox[{"(", "x", ")"}]}], "-", 
      RowBox[{"f", 
       RowBox[{"(", 
        RowBox[{"x", "+", 
         RowBox[{"2", "h"}]}], ")"}]}]}], 
     RowBox[{"2", "h"}]], "+", 
    RowBox[{
     FractionBox["1", "3"], 
     SuperscriptBox["h", "2"], 
     SuperscriptBox["f", 
      RowBox[{"(", "3", ")"}]], 
     RowBox[{"(", "\[Xi]", ")"}]}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "0581a88a-1655-4151-9fea-a69bfd07807c"],
 ".  Note that the error term is as predicted."
}], "Text",
 CellChangeTimes->{{3.7903808250338573`*^9, 3.790380835103077*^9}, 
   3.790380921031178*^9, {3.7903810594003363`*^9, 3.790381124516274*^9}, {
   3.790381199843285*^9, 3.79038122744425*^9}, {3.791152647743779*^9, 
   3.791152888906271*^9}, {3.791152921706641*^9, 3.7911529611522903`*^9}, {
   3.7911529974167557`*^9, 3.791153100109227*^9}, {3.791153146512333*^9, 
   3.7911535024672832`*^9}, {3.7911537502048807`*^9, 3.791153931369725*^9}, {
   3.791153986825407*^9, 3.7911540587380333`*^9}, {3.7911551121196537`*^9, 
   3.7911552133463573`*^9}, {3.791155326247985*^9, 3.791155596847046*^9}, {
   3.791155639840066*^9, 3.7911558470268393`*^9}, {3.791155888570118*^9, 
   3.791155916394059*^9}, {3.791159553822833*^9, 3.791159560280714*^9}, {
   3.7911596089357357`*^9, 
   3.791159609214706*^9}},ExpressionUUID->"9b67528f-47d5-4b8d-a0f1-\
765cf0144039"],

Cell[TextData[{
 StyleBox["\nExercise 5\n\n", "Subsubsection",
  FontSize->24],
 StyleBox["Averaging the forward-difference formula f\[CloseCurlyQuote](x) \
\[TildeTilde] [ f (x + h) \[Minus] f (x)]/h and the backward-difference \
formula f\[CloseCurlyQuote](x) \[TildeTilde] [ f (x) \[Minus] f (x \[Minus] \
h)]/h, each with error term O(h), results in the central-difference formula f\
\[CloseCurlyQuote](x) \[TildeTilde] [ f (x + h) \[Minus] f (x \[Minus] \
h)]/(2h) with error O(", "Chapter",
  FontSize->18],
 Cell[BoxData[
  SuperscriptBox["h", "2"]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "9f6247b1-d6ff-478b-9161-b0d8a688aee5"],
 StyleBox["). Show why. Hint: Determine at least the first term in the error \
series for each formula.\n\nf\[CloseCurlyQuote](x) \[TildeTilde] [ f (x + h) \
\[Minus] f (x)]/h\nf\[CloseCurlyQuote](x) \[TildeTilde] [ f (x) \[Minus] f (x \
\[Minus] h)]/h\n", "Chapter",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   StyleBox[
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", "x", ")"}]}], "Chapter",
    FontSize->18], 
   StyleBox["  ", "Chapter",
    FontSize->18], "=", 
   FractionBox["1", "2"]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "4e70d000-5fa6-4a8d-971e-ebb9e59c1bc7"],
 " [",
 StyleBox["f\[CloseCurlyQuote](x)+f\[CloseCurlyQuote](x)", "Chapter",
  FontSize->18],
 "]\n             ",
 Cell[BoxData[
  RowBox[{
   StyleBox["\[TildeTilde]", "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    StyleBox[
     RowBox[{
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", 
         StyleBox["+", "Chapter",
          FontSize->18], " ", "h"}], ")"}]}], "-", 
      RowBox[{"f", 
       RowBox[{"(", 
        StyleBox[
         StyleBox["x", "Chapter",
          FontSize->18], "Chapter",
         FontSize->18], 
        StyleBox[
         StyleBox[")", "Chapter",
          FontSize->18], "Chapter",
         FontSize->18]}]}], "+", " ", 
      RowBox[{"f", " ", 
       RowBox[{"(", "x", ")"}]}], " ", "\[Minus]", " ", 
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", "\[Minus]", " ", "h"}], ")"}]}]}], "Chapter",
     FontSize->18], 
    StyleBox["]", "Chapter",
     FontSize->18]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "a0b9488d-83fd-4209-9edf-11ea14c3eb16"],
 "\n             ",
 Cell[BoxData[
  RowBox[{
   StyleBox["=", "Chapter",
    FontSize->18], 
   StyleBox[" ", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    StyleBox[
     RowBox[{
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", 
         StyleBox["+", "Chapter",
          FontSize->18], " ", "h"}], ")"}]}], "\[Minus]", " ", 
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", "\[Minus]", " ", "h"}], ")"}]}]}], "Chapter",
     FontSize->18], 
    StyleBox["]", "Chapter",
     FontSize->18]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "52dabe5e-8fa0-4d23-b0b1-8efdb6cda1c4"],
 StyleBox["\n\nf (x + h) - f(x) = hf\[CloseCurlyQuote](x) + ", "Chapter",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox[
     SuperscriptBox["h", "2"], "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], " ", "+", " ", 
   RowBox[{
    FractionBox[
     SuperscriptBox["h", "3"], "6"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "1"], ")"}]}]}]],
  CellChangeTimes->{3.7906165341057777`*^9},ExpressionUUID->
  "79844e95-101f-4f21-8ff2-c1dd2cb38995"],
 StyleBox["\nf (x \[Minus] h) - f(x) = -hf\[CloseCurlyQuote](x) + ", "Chapter",
  FontSize->18],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox[
     SuperscriptBox["h", "2"], "2"], 
    SuperscriptBox["f", 
     RowBox[{"(", "2", ")"}]], 
    RowBox[{"(", "x", ")"}]}], "-", 
   RowBox[{
    FractionBox[
     SuperscriptBox["h", "3"], "6"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "2"], ")"}]}]}]],
  CellChangeTimes->{3.7906165341057777`*^9},ExpressionUUID->
  "ff9bd007-86cc-45d1-ba29-8d708eca45f8"],
 "\n\nNow we can expand our last chain of equalities, as follows.",
 StyleBox["\n\n", "Subsubsection",
  FontSize->24],
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    StyleBox[
     RowBox[{
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", 
         StyleBox["+", "Chapter",
          FontSize->18], " ", "h"}], ")"}]}], "\[Minus]", " ", 
      RowBox[{"f", " ", 
       RowBox[{"(", 
        RowBox[{"x", " ", "\[Minus]", " ", "h"}], ")"}]}]}], "Chapter",
     FontSize->18], 
    StyleBox["]", "Chapter",
     FontSize->18]}], 
   StyleBox["=", "Chapter",
    FontSize->18], 
   RowBox[{
    FractionBox["1", 
     RowBox[{"2", "h"}]], "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       StyleBox[
        RowBox[{
         RowBox[{"hf", "'"}], 
         RowBox[{"(", "x", ")"}]}], "Chapter",
        FontSize->18], 
       StyleBox[" ", "Chapter",
        FontSize->18], 
       StyleBox["+", "Chapter",
        FontSize->18], 
       StyleBox[" ", "Chapter",
        FontSize->18], 
       RowBox[{
        FractionBox[
         SuperscriptBox["h", "2"], "2"], 
        SuperscriptBox["f", 
         RowBox[{"(", "2", ")"}]], 
        RowBox[{"(", "x", ")"}]}], " ", "+", " ", 
       RowBox[{
        FractionBox[
         SuperscriptBox["h", "3"], "6"], 
        SuperscriptBox["f", 
         RowBox[{"(", "3", ")"}]], 
        RowBox[{"(", 
         SubscriptBox["\[Xi]", "1"], ")"}]}]}], ")"}], 
     StyleBox["\[Minus]", "Chapter",
      FontSize->18], 
     StyleBox[
      RowBox[{"(", 
       RowBox[{
        StyleBox[
         RowBox[{
          RowBox[{"-", 
           RowBox[{"hf", "'"}]}], 
          RowBox[{"(", "x", ")"}]}], "Chapter",
         FontSize->18], 
        StyleBox[" ", "Chapter",
         FontSize->18], 
        StyleBox["+", "Chapter",
         FontSize->18], 
        StyleBox[" ", "Chapter",
         FontSize->18], 
        RowBox[{
         FractionBox[
          SuperscriptBox["h", "2"], "2"], 
         SuperscriptBox["f", 
          RowBox[{"(", "2", ")"}]], 
         RowBox[{"(", "x", ")"}]}], "-", 
        RowBox[{
         FractionBox[
          SuperscriptBox["h", "3"], "6"], 
