   RowBox[{
    FractionBox[
     SuperscriptBox["h", "3"], "6"], 
    SuperscriptBox["f", 
     RowBox[{"(", "3", ")"}]], 
    RowBox[{"(", 
     SubscriptBox["\[Xi]", "1"], ")"}]}]}]],
  CellChangeTimes->{3.7906165341057777`*^9},ExpressionUUID->
  "174f7b3d-4994-4e03-acc3-3f35bc39f368"],
 ",",
 StyleBox["\n",
  FontSize->18],
 StyleBox["        f(x-h) - f(x) = -hf\[CloseCurlyQuote](x) + ", "Chapter",
