       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", 
        SubscriptBox["\[Xi]", "2"], ")"}]}]}], "]"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "204a05d8-f073-4a5c-ba84-5169e73a612b"],
 " \n\n              So f(x+2h)-f(x-2h) = ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"4", 
    RowBox[{"hf", "'"}], 
    RowBox[{"(", "x", ")"}]}], "+", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"3", "!"}]], 
    RowBox[{
     SuperscriptBox["h", "3"], "[", 
     RowBox[{
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", 
        SubscriptBox["\[Xi]", "1"], ")"}]}], "+", 
      RowBox[{
       SuperscriptBox["f", 
        RowBox[{"(", "3", ")"}]], 
       RowBox[{"(", 
        SubscriptBox["\[Xi]", "2"], ")"}]}]}], "]"}]}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "94eececd-b104-42c7-ae9d-35b0224536c3"],
 "\n              \[TwoWayRule]  ",
 Cell[BoxData[
  RowBox[{"4", 
   RowBox[{"hf", "'"}], 
   RowBox[{"(", "x", ")"}]}]],
  CellChangeTimes->{3.790380970184108*^9},ExpressionUUID->
  "8199dcf4-b285-47a0-a07e-d989d5639b93"],
 " = f(x+2h)-f(x-2h) -",
 Cell[BoxData[
  RowBox[{
   FractionBox["8", 
    RowBox[{"3", "!"}]], 
   RowBox[{
    SuperscriptBox["h", "3"], "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["f", 
       RowBox[{"(", "3", ")"}]], 
