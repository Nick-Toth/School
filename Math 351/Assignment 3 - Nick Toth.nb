       RowBox[{"4", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", 
           FractionBox["1640", "3281"]}], ")"}], "2"]}], "+", "1"}]], "=", 
     RowBox[{"-", 
