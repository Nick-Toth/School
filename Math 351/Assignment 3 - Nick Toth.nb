    RowBox[{
     RowBox[{"-", 
      FractionBox["2", 
       RowBox[{"R", "+", "1"}]]}], "=", 
     RowBox[{"-", 
      FractionBox["2", "5"]}]}]}]}]],ExpressionUUID->
  "eeab7bca-cbf2-47d8-beb9-5dba4e2a302e"],
