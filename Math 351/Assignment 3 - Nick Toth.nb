 StyleBox["\nExercise 28\n", "Subsubsection",
  FontSize->24],
 "\nLet f(x)= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"3", 
     SuperscriptBox["x", "3"]}], "+", 
    SuperscriptBox["x", "2"], "-", 
    RowBox[{"15", "x"}], "+", "3"}], TraditionalForm]],ExpressionUUID->
  "6f2d2504-8eb5-4195-85ca-3cc54e8167ba"],
 ", and let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], "=", "1"}], TraditionalForm]],ExpressionUUID->
  "58e9f9a7-cd7a-4a30-b35d-ab9692097334"],
 ".\n\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", 
    RowBox[{"n", "+", "1"}]], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["x", "n"], "-", 
     FractionBox[
      RowBox[{"f", 
       RowBox[{"(", 
        SubscriptBox["x", "n"], ")"}]}], 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", 
        SubscriptBox["x", "n"], ")"}]}]]}], " ", "=", 
    RowBox[{
     SubscriptBox["x", "n"], "-", 
     FractionBox[
      RowBox[{" ", 
       FormBox[
        RowBox[{
         RowBox[{"3", 
          SuperscriptBox[
           SubscriptBox["x", "n"], "3"]}], "+", 
         SuperscriptBox[
          SubscriptBox["x", "n"], "2"], "-", 
         RowBox[{"15", 
          SubscriptBox["x", "n"]}], "+", "3"}],
        TraditionalForm]}], 
      FormBox[
       RowBox[{
        RowBox[{"9", 
         SuperscriptBox[
          SubscriptBox["x", "n"], "2"]}], "+", 
        RowBox[{"2", 
         SubscriptBox["x", "n"]}], "-", "15"}],
       TraditionalForm]], " ", " "}]}]}]],ExpressionUUID->
  "1d92437d-0c3c-4b00-ba8f-2e0e83221e38"],
 ",\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", "1"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["x", "0"], "-", 
     FractionBox[
      RowBox[{"f", 
       RowBox[{"(", 
        SubscriptBox["x", "0"], ")"}]}], 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", 
        SubscriptBox["x", "0"], ")"}]}]]}], " ", "=", 
    RowBox[{
     RowBox[{"1", "-", 
      FractionBox[
       RowBox[{" ", 
        FormBox[
         RowBox[{
          RowBox[{"3", "*", 
           SuperscriptBox["1", "3"]}], "+", 
          SuperscriptBox["1", "2"], "-", "15", "+", "3"}],
         TraditionalForm]}], 
       FormBox[
        RowBox[{
         RowBox[{"9", "*", 
          SuperscriptBox["1", "2"]}], "+", "2", "-", "15"}],
        TraditionalForm]]}], "=", 
     RowBox[{
      RowBox[{"1", "-", 
       FractionBox[
        RowBox[{" ", 
         FormBox[
          RowBox[{"-", "8"}],
          TraditionalForm]}], 
        RowBox[{"-", "4"}]]}], "=", 
      RowBox[{"-", "1", "  "}]}]}]}]}]],ExpressionUUID->
  "e1ecbaa1-542c-416f-a558-af852d3109ab"],
 ",\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", "2"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["x", "1"], "-", 
     FractionBox[
      RowBox[{"f", 
       RowBox[{"(", 
        SubscriptBox["x", "1"], ")"}]}], 
      RowBox[{
       RowBox[{"f", "'"}], 
       RowBox[{"(", 
        SubscriptBox["x", "1"], ")"}]}]]}], " ", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "1"}], "-", 
      FractionBox[
       RowBox[{" ", 
        FormBox[
         RowBox[{
          RowBox[{"3", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "1"}], " ", ")"}], "3"]}], "+", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], " ", ")"}], "2"], "-", 
          RowBox[{"15", 
           RowBox[{"(", 
            RowBox[{"-", "1"}], " ", ")"}]}], "+", "3"}],
         TraditionalForm]}], 
       FormBox[
        RowBox[{
         RowBox[{"9", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], " ", ")"}], "2"]}], "+", 
         RowBox[{"2", 
          RowBox[{"(", 
           RowBox[{"-", "1"}], " ", ")"}]}], "-", "15"}],
        TraditionalForm]]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "1"}], "-", 
       FractionBox[
        RowBox[{" ", 
         FormBox[
          RowBox[{
           RowBox[{"-", "3"}], "+", "1", "-", "15", "+", "3"}],
          TraditionalForm]}], 
        FormBox[
         RowBox[{"9", "-", "2", "-", "15"}],
         TraditionalForm]]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "1"}], "-", 
        FractionBox[
         RowBox[{" ", 
          FormBox[
           RowBox[{"-", "14"}],
           TraditionalForm]}], 
         FormBox[
          RowBox[{"-", "8"}],
          TraditionalForm]]}], "=", 
       RowBox[{"-", 
        FractionBox[
         FormBox["22",
          TraditionalForm], 
         FormBox["8",
          TraditionalForm]], "  "}]}]}]}]}]}]],ExpressionUUID->
  "c206989c-8022-4e36-a25c-be1127347c36"],
