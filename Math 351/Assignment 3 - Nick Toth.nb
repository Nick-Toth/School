Cell[TextData[{
 StyleBox["Exercise 17", "Subsubsection",
  FontSize->24],
 StyleBox["\n", "Subsubsection"],
 "\nIf Newton\[CloseCurlyQuote]s method is used on f(x) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "5"], "-", 
    SuperscriptBox["x", "3"], "+", "3"}], TraditionalForm]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "90144f1d-41d2-4a8a-9245-d798e17c140c"],
 " and ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", "n"], " "}]],ExpressionUUID->
  "3039db0b-d2d8-4f54-be07-4ddc9bed3514"],
 "= 1, what is ",
 Cell[BoxData[
  SubscriptBox["x", 
   RowBox[{"n", "+", "1"}]]],ExpressionUUID->
  "21653e1b-449c-45d0-8166-0fc9e56cbb2d"],
 "?\n\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", 
    RowBox[{"n", "+", "1"}]], "=", 
   RowBox[{
    SubscriptBox["x", "n"], "-", 
    FractionBox[
     RowBox[{"f", 
      RowBox[{"(", 
       SubscriptBox["x", "n"], ")"}]}], 
     RowBox[{
      RowBox[{"f", "'"}], 
      RowBox[{"(", 
       SubscriptBox["x", "n"], ")"}]}]]}]}]],ExpressionUUID->
  "228d2b73-7c64-461f-bde6-4dcc205a67ee"],
 " = ",
 Cell[BoxData[
  RowBox[{"1", "-", 
   FractionBox[
    RowBox[{"f", 
     RowBox[{"(", "1", ")"}]}], 
    RowBox[{
     RowBox[{"f", "'"}], 
     RowBox[{"(", "1", ")"}]}]]}]],ExpressionUUID->
  "a1e409b9-bd9a-49e8-b5ee-e5dde7907323"],
 "= ",
 Cell[BoxData[
  RowBox[{"1", "-", 
   FractionBox[
    FormBox[
     RowBox[{
      SuperscriptBox["1", "5"], "-", 
      SuperscriptBox["1", "3"], "+", "3"}],
     TraditionalForm], 
    FormBox[
     RowBox[{
      RowBox[{"5", 
       SuperscriptBox[
        RowBox[{"(", "1", ")"}], "4"]}], "-", 
      RowBox[{"3", 
       SuperscriptBox[
        RowBox[{"(", "1", ")"}], "2"]}]}],
     TraditionalForm]]}]],ExpressionUUID->
  "1cd89692-6b20-4908-8cc0-15f9c652de5b"],
 "= ",
 Cell[BoxData[
  RowBox[{"1", "-", 
