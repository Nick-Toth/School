     RowBox[{"2", 
      SubscriptBox["x", "3"]}], 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["Rx", "3"], "2"], "+", "1"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"2", 
       RowBox[{"(", 
        RowBox[{"-", 
         FractionBox["1640", "3281"]}], ")"}]}], 
