Cell[TextData[{
 StyleBox["\n", "Subsubsection"],
 StyleBox["Exercise 4", "Subsubsection",
  FontSize->24],
 StyleBox["\n", "Subsubsection"],
 "\nCalculate the positive roots of  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "4"], "+", 
    RowBox[{"2", 
     SuperscriptBox["x", "3"]}], "-", 
    RowBox[{"7", 
     SuperscriptBox["x", "2"]}], "+", "3"}], TraditionalForm]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "9242aa28-7b5c-421b-bf3a-e80995ce593c"],
 " correct to 2 decimal places.\n\nLet f(x) = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "4"], "+", 
    RowBox[{"2", 
     SuperscriptBox["x", "3"]}], "-", 
    RowBox[{"7", 
     SuperscriptBox["x", "2"]}], "+", "3"}], TraditionalForm]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "61c970c1-3f95-4ebe-9415-f9e6ae310d89"],
 ",  f\[CloseCurlyQuote](x) = 4",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "3"], "+", 
    RowBox[{"6", 
     SuperscriptBox["x", "2"]}], "-", 
    RowBox[{"14", "x"}]}], TraditionalForm]],
  CellChangeTimes->{{3.7874407041988077`*^9, 3.7874407598841343`*^9}, {
   3.7874408036755466`*^9, 3.787440979877481*^9}, {3.787441015148995*^9, 
   3.78744106820081*^9}},ExpressionUUID->
  "aece8ad6-852d-433b-a90b-f7d6065874de"],
 ",\n",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["x", 
    RowBox[{"n", "+", "1"}]], " ", "=", " ", 
   RowBox[{
    SubscriptBox["x", "n"], "-", 
    FractionBox[
     RowBox[{"f", "(", 
      SubscriptBox["x", "n"], ")"}], 
     RowBox[{
      RowBox[{"f", "'"}], 
      RowBox[{"(", 
       SubscriptBox["x", "n"], ")"}]}]]}]}]],ExpressionUUID->
  "8844dab6-2162-4b8b-af81-e80dc987fd24"],
 "\n           ",
 Cell[BoxData[
  RowBox[{"=", " ", 
   RowBox[{
    SubscriptBox["x", "n"], "-", 
    FractionBox[
     FormBox[
      RowBox[{
       SuperscriptBox[
        SubscriptBox["x", "n"], "4"], "+", 
       RowBox[{"2", 
        SuperscriptBox[
         SubscriptBox["x", "n"], "3"]}], "-", 
       RowBox[{"7", 
        SuperscriptBox[
         SubscriptBox["x", "n"], "2"]}], "+", "3"}],
      TraditionalForm], 
     RowBox[{"4", 
      FormBox[
       RowBox[{
        SuperscriptBox[
         SubscriptBox["x", "n"], "3"], "+", 
        RowBox[{"6", 
         SuperscriptBox[
          SubscriptBox["x", "n"], "2"]}], "-", 
        RowBox[{"14", 
         SubscriptBox["x", "n"]}]}],
       TraditionalForm]}]]}]}]],ExpressionUUID->
  "4671e711-54d1-4205-ba50-5413e19ec419"],
 "\n           ",
 Cell[BoxData[
  RowBox[{"=", " ", 
   FractionBox[
    FormBox[
     RowBox[{
      RowBox[{"4", 
       FormBox[
        RowBox[{
         SuperscriptBox[
          SubscriptBox["x", "n"], "4"], "+", 
         RowBox[{"6", 
          SuperscriptBox[
           SubscriptBox["x", "n"], "3"]}], "-", 
         RowBox[{"14", 
          SuperscriptBox[
           SubscriptBox["x", "n"], "2"]}]}],
        TraditionalForm]}], "-", 
      SuperscriptBox[
       SubscriptBox["x", "n"], "4"], "-", 
      RowBox[{"2", 
       SuperscriptBox[
        SubscriptBox["x", "n"], "3"]}], "+", 
      RowBox[{"7", 
       SuperscriptBox[
        SubscriptBox["x", "n"], "2"]}], "-", "3"}],
     TraditionalForm], 
    RowBox[{"4", 
     FormBox[
      RowBox[{
       SuperscriptBox[
        SubscriptBox["x", "n"], "3"], "+", 
       RowBox[{"6", 
        SuperscriptBox[
         SubscriptBox["x", "n"], "2"]}], "-", 
       RowBox[{"14", 
        SubscriptBox["x", "n"]}]}],
      TraditionalForm]}]]}]],ExpressionUUID->
  "5eafa5c1-978b-46e3-a98f-299f4fb89359"],
 "\n             = ",
 Cell[BoxData[
  FractionBox[
   RowBox[{" ", 
